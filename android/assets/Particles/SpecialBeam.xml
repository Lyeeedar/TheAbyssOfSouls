<Effect>
	<MoveSpeed>0.1</MoveSpeed>
	<Emitters>
		<Emitter>
			<Name>HeadGlow</Name>
			<Space>Local</Space>
			<Shape>Circle</Shape>
			<Width>0.2</Width>
			<Height>0.2</Height>
			<Offset>0,0.2</Offset>
			<Direction>Random</Direction>
			<ParticleSpeed>0,0</ParticleSpeed>
			<ParticleRotation>0,1</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|5</Keyframe>
			</RateKeyframes>
			<Particles>
				<Particle>
					<Lifetime>1,1</Lifetime>
					<BlendMode>Additive</BlendMode>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/glow</Keyframe>
						</Stream>
					</TextureKeyframes>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0.5|1</Keyframe>
						</Stream>
					</SizeKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|0.2</Keyframe>
						</Stream>
					</AlphaKeyframes>
				</Particle>
			</Particles>
		</Emitter>
		<Emitter>
			<Name>Head</Name>
			<IsCollisionEmitter>true</IsCollisionEmitter>
			<Space>Local</Space>
			<Shape>Cone</Shape>
			<Width>25</Width>
			<Height>0.2</Height>
			<Area>Border</Area>
			<Direction>Up</Direction>
			<ParticleSpeed>0,0</ParticleSpeed>
			<ParticleRotation>0,1</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|5</Keyframe>
			</RateKeyframes>
			<Particles>
				<Particle>
					<Lifetime>0.5,0.5</Lifetime>
					<BlendMode>Multiplicative</BlendMode>
					<VelocityAligned>true</VelocityAligned>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/droplet</Keyframe>
						</Stream>
					</TextureKeyframes>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|1|1</Keyframe>
						</Stream>
					</SizeKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|1</Keyframe>
						</Stream>
					</AlphaKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>